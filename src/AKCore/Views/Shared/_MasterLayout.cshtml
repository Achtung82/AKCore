@using System.Threading.Tasks
@using AKCore.DataModel
<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Alte Kamereren - @ViewData["Title"]</title>
    <meta name="description" content="Musikalliansen Alte Kamereren består av studenter från Lunds Universitet och Lunds Tekniska Högskola. I alliansen ingår den förträffliga studentorkestern Alte Kamereren och den underbara Kamrérbaletten.">
    <link href="https://fonts.googleapis.com/css?family=Roboto:300,400,500" rel="stylesheet">
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.5.0/css/font-awesome.min.css">
    <link rel="stylesheet" href="~/css/akstyle.css" asp-append-version="true">
</head>
<body>
    @await Component.InvokeAsync("Header")
    <div class="container body-content">
        <div class="content-wrapper">
            @RenderBody()
        </div>
    </div>
    @Html.Partial("_LoginModal")
    <script src="https://code.jquery.com/jquery-3.2.1.min.js"
            integrity="sha256-hwg4gsxgFZhOsEEamdOYGBf13FyQuiTwlAQgxVSNgt4="
            crossorigin="anonymous"></script>
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.7/js/bootstrap.min.js" integrity="sha384-Tc5IQib027qvyjSMfHjOMaLkfuWVxZxUPnCJA7l2mCWNIpG9mGCD8wGNIcPD7Txa" crossorigin="anonymous"></script>
    <script src="~/js/main.js" asp-append-version="true"></script>
    @RenderSection("scripts", required: false)
</body>
</html>
